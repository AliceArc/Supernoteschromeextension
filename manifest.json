    "version": "1.0",
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png" },
    "description": "Supernotes Chrome Extension",
    "permissions": ["storage"],
    "background": {
      "scripts": ["background.js"],
