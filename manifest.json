 {
    "name": "Supernotes - Collaborative Notetaking App",
    "description": "The collaborative notetaking app chrome extension - immediately take notes wherever you are.",
    "version": "1.0",
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png" },
    "description": "Supernotes Chrome Extension",
    "permissions": ["storage"],
    "author": "Supernotes",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_icon":{
         "16": "images/icon16.png",
         "24": "images/icon24.png",
         "32": "images/icon32.png"
       },
       "default_title": "Supernotes"
     },
    "commands": {
       "_execute_browser_action": {
            "suggested_key": {
              "windows": "Ctrl+Shift+S",
              "mac": "Command+Shift+S",
              "chromeos": "Ctrl+Shift+S",
              "linux": "Ctrl+Shift+S"
            }
          }
     },
    "permissions": [ ],
    "manifest_version": 2,
    "short_name": "Supernotes"
  }
